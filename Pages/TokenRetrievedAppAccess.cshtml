@page
@model CIS2Auth.Pages.TokenRetrievedAppAccessModel
@{
	ViewData["Title"] = "Token Retrieved Application Access";
}
<br />
<h2 class="text-info">CIS2 Token Retrieved Application Access</h2>
<br />

<div class="border container" style="padding: 30px;">
	<div class="form-group row">
		<div class="col-3">
			<label asp-for="Token.access_token"></label>
		</div>
		<div class="col-9">
			<input asp-for="Token.access_token" class="form-control" value="@Model.Token.access_token"/>
		</div>
	</div>
		
	<div class="form-group row">
		<div class="col-3">
			<label asp-for="Token.expires_in"></label>
		</div>
		<div class="col-9">
			<input asp-for="Token.expires_in" class="form-control" value="@Model.Token.expires_in"/>
		</div>
	</div>
		
	<div class="form-group row">
		<div class="col-3">
			<label asp-for="Token.token_type"></label>
		</div>
		<div class="col-9">
			<input asp-for="Token.token_type" class="form-control" value="@Model.Token.token_type"/>
		</div>
	</div>
</div>